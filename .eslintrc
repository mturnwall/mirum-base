{
    "globals": {},
    "env": {
        "es6": true,
        "browser": true,
        "jquery": true
    },
    "rules": {
        /**
         * Style
         */
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        /**
         * Warnings
         */
        "no-console": 1,
        "no-unused-vars": 1
    },
    "extends": "eslint:recommended"
}
